	sub jja-deva [reph-deva reph_anusvara-deva] by jja-deva;
	sub ddda-deva [reph-deva reph_anusvara-deva] by ddda-deva;
	sub bba-deva [reph-deva reph_anusvara-deva] by bba-deva;
	sub la-deva.loclMAR [reph-deva reph_anusvara-deva] by la-deva.loclMAR;
	sub sha-deva.loclMAR [reph-deva reph_anusvara-deva] by sha-deva.loclMAR;
	sub ya-deva.post [reph-deva reph_anusvara-deva] by ya-deva.post;
	sub va-deva.post [reph-deva reph_anusvara-deva] by va-deva.post;
	#sub na-deva.post2 [reph-deva reph_anusvara-deva] by na-deva.post2;
	#sub ra-deva.post2 [reph-deva reph_anusvara-deva] by ra-deva.post2;
} removeReph;

