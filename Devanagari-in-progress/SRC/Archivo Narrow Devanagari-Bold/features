	sub tha-deva [reph-deva reph_anusvara-deva] by tha-deva;
	sub da-deva [reph-deva reph_anusvara-deva] by da-deva;
	sub dha-deva [reph-deva reph_anusvara-deva] by dha-deva;
	sub na-deva [reph-deva reph_anusvara-deva] by na-deva;
	sub nnna-deva [reph-deva reph_anusvara-deva] by nnna-deva;
	sub pa-deva [reph-deva reph_anusvara-deva] by pa-deva;
	sub pha-deva [reph-deva reph_anusvara-deva] by pha-deva;
	sub ba-deva [reph-deva reph_anusvara-deva] by ba-deva;
	sub bha-deva [reph-deva reph_anusvara-deva] by bha-deva;
	sub ma-deva [reph-deva reph_anusvara-deva] by ma-deva;
	sub ya-deva [reph-deva reph_anusvara-deva] by ya-deva;
	sub ra-deva [reph-deva reph_anusvara-deva] by ra-deva;
	sub rra-deva [reph-deva reph_anusvara-deva] by rra-deva;
	sub la-deva [reph-deva reph_anusvara-deva] by la-deva;
	sub lla-deva [reph-deva reph_anusvara-deva] by lla-deva;
	sub llla-deva [reph-deva reph_anusvara-deva] by llla-deva;
	sub va-deva [reph-deva reph_anusvara-deva] by va-deva;
	sub sha-deva [reph-deva reph_anusvara-deva] by sha-deva;
	sub ssa-deva [reph-deva reph_anusvara-deva] by ssa-deva;
	sub sa-deva [reph-deva reph_anusvara-deva] by sa-deva;
	sub ha-deva [reph-deva reph_anusvara-deva] by ha-deva;
	sub qa-deva [reph-deva reph_anusvara-deva] by qa-deva;
	sub khha-deva [reph-deva reph_anusvara-deva] by khha-deva;
	sub ghha-deva [reph-deva reph_anusvara-deva] by ghha-deva;
	sub za-deva [reph-deva reph_anusvara-deva] by za-deva;
	sub dddha-deva [reph-deva reph_anusvara-deva] by dddha-deva;
	sub rha-deva [reph-deva reph_anusvara-deva] by rha-deva;
	sub fa-deva [reph-deva reph_anusvara-deva] by fa-deva;
	sub yya-deva [reph-deva reph_anusvara-deva] by yya-deva;
	sub gga-deva [reph-deva reph_anusvara-deva] by gga-deva;
	sub jja-deva [reph-deva reph_anusvara-deva] by jja-deva;
	sub ddda-deva [reph-deva reph_anusvara-deva] by ddda-deva;
	sub bba-deva [reph-deva reph_anusvara-deva] by bba-deva;
	#sub jha-deva.NEP [reph-deva reph_anusvara-deva] by jha-deva.NEP;
	#sub zha-deva.NEP [reph-deva reph_anusvara-deva] by zha-deva.NEP;
} removeReph;
