	sub ka-deva [reph-deva reph_anusvara-deva] by ka-deva;
	sub kha-deva [reph-deva reph_anusvara-deva] by kha-deva;
	sub ga-deva [reph-deva reph_anusvara-deva] by ga-deva;
	sub gha-deva [reph-deva reph_anusvara-deva] by gha-deva;
	sub nga-deva [reph-deva reph_anusvara-deva] by nga-deva;
