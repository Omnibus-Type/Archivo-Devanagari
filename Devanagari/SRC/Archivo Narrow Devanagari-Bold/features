sub ddadeva nuktadeva by dddhadeva;
sub khadeva nuktadeva by khhadeva;
sub yadeva nuktadeva by yyadeva;
} nukt ;

feature rphf {
sub radeva halantdeva by rephdeva;
} rphf ;

